tify-content: center;\n  align-items: center;\n}\n\n.hero-header {\n  position: sticky;\n  top: 0;\n  left: 0;\n  right: 0;\n  z-index: 1;\n  margin-top: 10%;\n \n}\n\n.hero {\n  position: relative;\n  transform: translate(-50%, -50%);\n  text-align: center;\n  color: #37ff00;\n  overflow: hidden;\n  width: 100%;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n \n}\n\n.typing-text {\n  display: inline-block;\n  border-right: 0.15em solid orange;\n  animation: typing 3s steps(40, end) forwards, blink-text 0.5s step-end infinite;\n  white-space: nowrap;\n  overflow: hidden;\n  width: 0;\n  font-weight: bold;\n  font-family:Arial, Helvetica, sans-serif;\n}\n.typing-text::after {\n  content: \"\";\n  position: absolute;\n  left: 0;\n  bottom: 0;\n  width: 0.15em;\n  height: 1.1em;\n  background-color: orange;\n  animation: cursor 2s steps(40, end) infinite;\n  font-family:Arial, Helvetica, sans-serif;\n}\n\n\n@keyframes typing {\n  from {\n    width: 0;\n  }\n  to {\n    width: 100%;\n  }\n}\n\n@keyframes blink-text {\n  from {\n    border-right-color: orange;\n  }\n  to {\n    border-right-color: transparent;\n  }\n}\n\n@keyframes cursor {\n  from, to {\n    opacity: 1;\n  }\n  50% {\n    opacity: 0;\n  }\n}\n\n\n@media screen and (min-width: 320px) and (max-width: 575px) {\n  .services {\n    text-align: center;\n  }\n\n  .service {\n    width: 90%; \n    margin: 1rem auto;\n  }\n\n  .service img {\n    max-width: 100%; \n    height: auto;\n  }\n}\n\n/* Media query para telas médias (a partir de 576px até 991px) */\n@media screen and (min-width: 576px) and (max-width: 991px) {\n  .services {\n    text-align: center;\n  }\n\n  .service {\n    width: 45%; \n    margin: 1rem;\n  }\n\n  .service img {\n    max-width: 100%; \n    height: auto;\n  }\n}\n\n/* Media query para telas maiores (a partir de 992px até 1800px) */\n@media screen and (min-width: 992px) and (max-width: 1800px) {\n \n  .service img {\n    max-width: 100%; \n    height: auto;\n  }\n}\n\